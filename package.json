    "@quasibit/eleventy-plugin-schema": "^1.11.0",
    "css-loader": "^6.9.0",
    "mini-css-extract-plugin": "^2.7.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "type": "module"
}
